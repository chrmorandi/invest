{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": [
        "yii2",
        "framework"
    ],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7",
        "yiisoft/yii2": "*",
        "npm-asset/font-awesome": "~5.1",
        "yiisoft/yii2-bootstrap4": "*",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "kartik-v/yii2-helpers": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-widget-affix": "dev-master",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-datecontrol": "*",
        "kartik-v/yii2-checkbox-x": "*",
        "kartik-v/yii2-builder": "*",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-money": "*",
        "kartik-v/yii2-password": "*",
        "kartik-v/yii2-dynagrid": "*",
        "kartik-v/yii2-tabs-x": "*",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-widget-select2": "^2.2",
        "kartik-v/yii2-number": "@dev",
        "miloschuman/yii2-highcharts-widget": "^7.0",
        "php-ai/php-ml": "*",
        "webzop/yii2-notifications": "*",
        "yiisoft/yii2-queue": "*",
        "toriphes/yii2-console-runner": "1.2.1",
        "wisembly/elephant.io": "^3.3",
        "kartik-v/yii2-icons": "@dev",
        "sjaakp/yii2-collapse": "^1.1",
        "smalot/pdfparser": "*",
        "kartik-v/bootstrap-fileinput": "dev-master",
        "hail812/yii2-adminlte3": "~1.1",
        "kartik-v/yii2-bootstrap4-dropdown": "@dev",
        "brick/math": "^0.12.1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.1",
        "codeception/verify": "*",
        "codeception/specify": "*",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-db": "^2.1",
        "codeception/module-asserts": "*",
        "codeception/module-yii2": "*",
        "codeception/module-webdriver": "*",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "codeception/module-filesystem": "^2.0",
        "phpstan/phpstan": "^1.9",
        "nunomaduro/phpinsights": "^2.6"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "app\\": "src",
            "tests\\": "tests/"
        }
    }
}
